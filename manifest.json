{
    "manifest_version": 3,
    "name": "Leads Tracker",
    "version": "1.0.0",
    "description": "A small extension used to store Links of your choice",
    "action":{
        "default_popup": "index.html",
         "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "permissions": [
        "tabs"
    ]
  }